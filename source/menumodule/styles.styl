// menu styles
#menu
    background-color rgba(0, 0, 0, 0.2)
    padding 10px
    width: 100%
    margin-bottom 20px

#go-icon
    width: 40px
    height: 40px
    background-image: url(/img/arrow-white.svg)
    background-size: contain
    background-position: center
    background-repeat: no-repeat
    transform: rotate(90deg)
    cursor: pointer
