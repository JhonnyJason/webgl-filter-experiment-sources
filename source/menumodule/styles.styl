// menu styles
#menu
    display flex
    justify-content: center
    background-color rgba(0, 0, 0, 0.2)
    box-sizing: border-box
    padding 10px
    width: 100%
    margin-bottom 20px
    
    .active
        border solid 3px #92B158

#menu > svg
    border solid 3px transparent
    width: 40px
    height: 40px
    margin-left 20px
    color white
    cursor: pointer

#filter-settings-icon
    margin-left 200px !important

